<?php
class Example extends PHPUnit_Extensions_SeleniumTestCase
{
  protected function setUp()
  {
    $this->setBrowser("*chrome");
    $this->setBrowserUrl("http://localhost/");
  }

  public function testMyTestCase()
  {
    $this->open("/addemp/smartvid/model/main.php");
    $this->click("id=signupNowBtn");
    $this->type("id=firstName", "Test");
    $this->type("id=lastName", "Tester");
    $this->type("css=div.col-sm-6.input-column > input[name=\"username\"]", "test123");
    $this->type("name=email", "test123@gmail.com");
    $this->type("name=password_1", "test");
    $this->type("name=password_2", "test");
    $this->click("//button[@type='button']");
    $this->click("css=div.checkbox");
    $this->click("id=acceptTerms");
    $this->click("name=reg_user");
    $this->waitForPageToLoad("30000");
    $this->click("id=uploadVideoButton");
    $this->click("id=createClassButton");
    $this->click("id=manageClassesButton");
    $this->click("id=manageVideosButton");
    $this->click("id=accountSettingsButton");
    $this->click("link=Home");
    $this->waitForPageToLoad("30000");
    $this->click("link=Logout");
    $this->waitForPageToLoad("30000");
  }
}
?>